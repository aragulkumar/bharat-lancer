# Environment variables for Docker Compose
# Copy this file to .env and fill in your values

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:password@ep-xxx.us-east-2.aws.neon.tech/bharathlancers

# JWT Secret (generate a strong random string)
JWT_SECRET=your-super-secret-jwt-key-change-this-min-32-characters

# Gemini API Key
GEMINI_API_KEY=your-gemini-api-key-here

# Frontend API URL (for production, use your backend URL)
VITE_API_URL=http://localhost:5000/api
